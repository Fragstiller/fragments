   "source": [
    "import yfinance as yf\n",
    "import pickle as pkl"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [],
   "source": [
    "goog = yf.Ticker(\"GOOG\")\n",
    "hist = goog.history(period=\"1y\")\n",
    "hist_list = list(zip(hist[\"Open\"], hist[\"High\"], hist[\"Low\"], hist[\"Close\"], hist[\"Volume\"]))\n",
    "with open(\"GOOG.pkl\", \"wb\") as f:\n",
    "    pkl.dump(hist_list, f)"
   ]
  }
